{
  "baseDirs": [
    ".tmp",
    "dev"
  ],
  "exts": {
    "include": ["js", "min.js"],
    "js": ["js", "min.js"],
    "css": ["css", "min.css"]
  },
  "dependencies": {
    "ngRoute": [
      "vendors/angular-route/angular-route.js"
    ],

    "todomvc": [
      "vendors/todomvc-common/base.css",
      "todomvc/css.css",
      "vendors/todomvc-common/base.js",
      "vendors/angular/angular.js",
      "todomvc/js.js"
    ],
    "todomvc-todoCtrl": [
      "todomvc/controllers/todoCtrl.js"
    ],
    "todomvc-todoEscape": [
      "todomvc/directives/todoEscape.js"
    ],
    "todomvc-todoFocus": [
      "todomvc/directives/todoFocus.js"
    ],
    "todomvc-todoStorage": [
      "todomvc/services/todoStorage.js"
    ],

    "ui.router": [
      "vendors/angular-ui-router/release/angular-ui-router.js"
    ],
    
    "base": [
      "vendors/angular/angular.js",
      "vendors/lodash/lodash.js",
      "common/base/js.js"
    ],

    "app": [
      "app/css.css",
      "app/js.js"
    ],
    "app.home": [
      "app/home/css.css",
      "app/home/js.js"
    ],
    "app.about": [
      "app/about/css.css",
      "app/about/js.js"
    ],
    "app.user": [
      "app/user/css.css",
      "app/user/js.js"
    ],
    "app.user.detail": [
      "app/user/detail/css.css",
      "app/user/detail/js.js"
    ],
    "com-header": [
      "common/com-header/css.css",
      "common/com-header/js.js"
    ]
  },
  "priorities": {
    "vendors/angular/angular.js": 1
  },
  "groups": [{
    "name": "libs",
    "regex": ".*.min.css"
  }, {
    "name": "css",
    "regex": ".*.css"
  }, {
    "name": "libs",
    "regex": ".*.min.js"
  }, {
    "name": "js",
    "regex": ".*.js"
  }]
}
