{
  "mode": "auto",
  "baseDirs": [
    ".tmp",
    "dev"
  ],
  "exts": {
    "include": ["js", "min.js"],
    "js": ["js", "min.js"],
    "css": ["css", "min.css"]
  },
  "dependencies": {
    "ngResource": [
      "/vendors/angular-resource/angular-resource.min.js"
    ],
    "ngSanitize": [
      "/vendors/angular-sanitize/angular-sanitize.min.js"
    ],
    "ui.router": [
      "/vendors/angular-ui-router/release/angular-ui-router.min.js"
    ],
    "app": [
      "/app/css.css",
      "/vendors/angular/angular.min.js",
      "/app/js.js"
    ]
  },
  "priorities": {

  },
  "groups": [{
    "name": "libs",
    "regex": ".*.min.css"
  }, {
    "name": "css",
    "regex": ".*.css"
  }, {
    "name": "libs",
    "regex": ".*.min.js"
  }, {
    "name": "js",
    "regex": ".*.js"
  }]
}